schema {
    query: QueryType
}

type QueryType {
    carInfo(carId: Int!): CarInfoType
    fuelInfo(typ: FuelType!): FuelPriceType
    allModels: [CarInfoType]!
}

enum FuelType {
    DIESEL
    BENZOL
}

type CarInfoType {
    id: Int!,
    name: String,
    consumption: Float,
    typ: FuelType
}

type FuelPriceType {
    price: Float!
}